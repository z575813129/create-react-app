{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "browsers": ["> 1%", "last 2 versions", "not ie <=8"]
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ],
        "@babel/transform-runtime",
        "@babel/plugin-transform-regenerator",
        "@babel/plugin-proposal-class-properties",
        "react-hot-loader/babel"
    ]
}